<div class="page-header">
  <%= link_to daily_totals_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_daily_total_path(@daily_total), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show daily_total</h1>
</div>

<dl class="dl-horizontal">
  <dt>Date:</dt>
  <dd><%= @daily_total.date %></dd>

  <dt>New card regs:</dt>
  <dd><%= @daily_total.new_card_regs %></dd>

  <dt>Daily rx users:</dt>
  <dd><%= @daily_total.daily_rx_users %></dd>

  <dt>Device registrations:</dt>
  <dd><%= @daily_total.device_registrations %></dd>

  <dt>Device activity:</dt>
  <dd><%= @daily_total.device_activity %></dd>

  <dt>Peoplescanning:</dt>
  <dd><%= @daily_total.peoplescanning %></dd>

  <dt>Items scanned:</dt>
  <dd><%= @daily_total.items_scanned %></dd>

</dl>
